{
  "name": "tyr",
  "version": "0.1.0",
  "bin": "lib/index.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc",
    "dev": "yarn build && node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkitsao/tyr"
  },
  "author": "Jackson kitsao <kitsaojackson22@gmail.com>",
  "bugs": {
    "url": "https://github.com/jkitsao/tyr/issues"
  },
  "homepage": "https://github.com/jkitsao/tyr",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "typescript": "^4.9.4"
  },
  "optionalDependencies": {
    "tyr-x86_64-linux": "0.1.0",
    "tyr-x86_64-macos": "0.1.0",
    "tyr-windows-windows ": "0.1.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "ignorePatterns": [
      "lib/*"
    ],
    "root": true
  }
}
